{

  "manifest_version": 3,
  "name": "Synced Scroll",
  "version": "2.1",

  "description": "Synchronized scroll in multiple windows simultaneously.",

  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {},
    "permissions": [
      "tabs",
      "scripting",
      "activeTab",
      "storage"
  ]

}